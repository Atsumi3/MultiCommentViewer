<Window x:Class="TwitchMessageProviderTestClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TwitchMessageProviderTestClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button Command="{Binding ConnectCommand}" Content="Button" HorizontalAlignment="Left" Margin="432,35,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="115,35,0,0" TextWrapping="Wrap" Text="{Binding ChannelName}" VerticalAlignment="Top" Width="300"/>
        <Label Content="チャンネル名" HorizontalAlignment="Left" Margin="27,37,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
